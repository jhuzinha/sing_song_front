{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:cypress/recommended"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "no-undef": "off",
      "react/prop-types": "off",
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
  }